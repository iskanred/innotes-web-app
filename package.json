{
	"name": "innotes-web-app",
	"version": "0.0.1",
	"private": true,
	"description": "Simple note taking web application",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/iskanred/innotes-web-app"
	},
	"bugs": {
		"url": "https://github.com/Iskanred/iu-frontend-course/issues"
	},
	"homepage": "https://github.com/Iskanred/iu-frontend-course#readme",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"prepare": "husky install"
	},
	"devDependencies": {
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-node": "^1.2.3",
		"@sveltejs/kit": "^1.5.0",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.1",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0",
		"vitest": "^0.25.3"
	},
	"type": "module",
	"dependencies": {
		"@svelteuidev/composables": "^0.11.1",
		"@svelteuidev/core": "^0.11.1",
		"@svelteuidev/dates": "^0.11.1",
		"dayjs": "^1.11.7",
		"radix-icons-svelte": "^1.2.1",
		"svelte-routing": "^1.6.0"
	}
}
